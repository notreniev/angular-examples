{
  "htmlWhitespaceSensitivity": "strict",
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "arrowParens": "always",
  "printWidth": 140,
  "useTabs": false,
  "jsxSingleQuote": false,
  "endOfLine": "auto",
  "jsxBracketSameLine": true,
  "quoteProps": "consistent",
  "proseWrap": "preserve"
}
